{
    "type": "oracle",
    "host": "${DB_HOST}",
    "port": 1521,
    "username": "${DB_USERNAME}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_DATABASE}",
    "synchronize": true,
    "logging": true,
    "entities": [
      "src/entity/**/*.ts"
    ],
    "migrations": [
      "src/migrations/**/*.ts"
    ],
    "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migrations"
    }
  }
  